{
    "name": "zaichaopan/access-granted",
    "description": "A Package to Add Role and Permission",
    "type": "library",
    "keywords": [
        "laravel",
        "role",
        "permission"
    ],
    "authors": [
        {
            "name": "zaichaopan",
            "email": "zaichaopan@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Zaichaopan\\AccessGranted\\": "src"
        }
    },
    "require": {
        "php": ">=7.1",
        "illuminate/support": "~5.5.0|~5.6.0"
    },
    "license": "MIT",
    "require-dev": {
        "orchestra/testbench": "^3.5.0",
        "phpunit/phpunit": "^7.2"
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php",
            "tests/stubs/User.php",
            "tests/HasPermissionsTraitTests.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zaichaopan\\AccessGranted\\AccessGrantedServiceProvider"
            ]
        }
    }
}
